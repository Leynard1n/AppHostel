<Window x:Class="AppHostel.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppHostel"
        mc:Ignorable="d"
        Title="LogWindow" Height="770" Width="1000" WindowStyle="None" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen" ShowInTaskbar="True" Icon="/house.png"> 
    <Grid MouseDown="Drag" Background="#787C40">
        <StackPanel>
            <Grid HorizontalAlignment="Right" Width="45">
                <Button x:Name="CloseButton2" Style="{DynamicResource CloseButtonMainWindow}" Click="CloseButton2_Click" Width="20" Height="20" Content="X" HorizontalAlignment="Right"/>
                <Button x:Name="MInimazeButton2" Style="{DynamicResource MiniMIzeButtonMainWindow}" Click="MInimazeButton2_Click" Width="20" Height="20" Content="—" HorizontalAlignment="Left"/>
            </Grid>
            <Frame x:Name="MainFrame" Background="white" NavigationUIVisibility="Hidden" Height="750" Navigated="MainFrame_Navigated"/>
        </StackPanel>
        

    </Grid>
</Window>
